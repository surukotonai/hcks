{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:app": "vite",
    "build:app": "vite build",
    "dev:wiki": "vitepress dev wiki",
    "build:wiki": "vitepress build wiki",
    "dev": "concurrently \"npm run dev:app\" \"npm run dev:wiki\"",
    "build": "npm run build:app && npm run build:wiki && mkdir -p dist/wiki && cp -R wiki/.vitepress/dist/* dist/wiki/"
  },
  "dependencies": {
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.1.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitepress": "^1.6.3",
    "vitepress-plugin-tabs": "^0.7.0"
  }
}
